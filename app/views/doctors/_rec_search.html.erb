<% if defined?@doctors %>
<div class="rec-search-result">
  <% @doctors.each do |doctor| %>
    <p>
    <%= doctor.first_name%> <%= doctor.last_name %>
    </p>
    <p>
      <%= doctor.specialty %>
    </p>
    <p>
      <%= doctor.city %>, <%= doctor.state %>
    </p>

    <%= button_to "Recommend", recommendations_new_path , method: :get, params: { doctor_id: doctor.id} %>

    <% end %>
</div>

<% end %>
