<div class="login-container">
  <%= form_tag(login_path, method: "post") do %>
    <div class="">
      <%= label_tag :Username%>
      <%= text_field_tag :username%>
    </div>
    <div class="">
      <%= label_tag :Password %>
      <%= password_field_tag :password %>
    </div>
      <%= submit_tag :login%>
  <% end %>
</div>
